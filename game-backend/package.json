{
  "name": "game-backend",
  "version": "1.0.0",
  "description": "Serverless Game backend",
  "main": "handler.ts",
  "scripts": {
    "build": "node --max-old-space-size=4096 ../node_modules/serverless/bin/serverless package",
    "deploy": "node --max-old-space-size=4096 ../node_modules/serverless/bin/serverless deploy",
    "start": "cross-env STAGE=\"local\" REDIS_HOST=\"localhost\" REDIS_PASSWORD=\"\" sls offline",
    "start:docker": "cross-env STAGE=\"local\" REDIS_HOST=\"redis\" REDIS_PASSWORD=\"\" sls offline --host 0.0.0.0"
  },
  "dependencies": {
    "@yingyeothon/actor-system": "^0.3.1",
    "@yingyeothon/actor-system-redis-support": "^0.3.0",
    "@yingyeothon/do-game-all-together": "1.0.0",
    "@yingyeothon/lambda-gamebase": "1.0.0",
    "@yingyeothon/logger": "^0.3.1",
    "@yingyeothon/naive-redis": "^0.3.2",
    "mem": "^8.1.1",
    "source-map-support": "^0.5.19",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@serverless/typescript": "^2.40.0",
    "@types/aws-lambda": "^8.10.76",
    "@types/jest": "^26.0.23",
    "@types/node": "15",
    "@types/serverless": "^1.78.26",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "aws-sdk": "2.804.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "serverless": "^2.40.0",
    "serverless-offline": "^7.0.0",
    "serverless-prune-plugin": "^1.5.0",
    "serverless-webpack": "^5.4.2",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.1.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "webpack": "^5.36.2",
    "webpack-bundle-analyzer": "^4.4.1",
    "wscat": "^4.0.1"
  },
  "author": "jaeyoung.choi <lactrious@gmail.com>",
  "license": "MIT"
}
